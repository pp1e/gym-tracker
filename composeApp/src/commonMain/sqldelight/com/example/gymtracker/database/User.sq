CREATE TABLE user (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    first_name TEXT NOT NULL,
    last_name TEXT NOT NULL,
    email TEXT NOT NULL UNIQUE,
    phone TEXT UNIQUE,
    height REAL,
    weight REAL,
    chest REAL,
    waist REAL,
    hips REAL,
    created_at INTEGER NOT NULL,
    updated_at INTEGER,
    password TEXT NOT NULL
);
